<table id="example2" class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>No.</th>
      <th>Nama Product</th>
      <th>Jumlah Product</th>
      <th>Aksi</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i = 0; i < quantity.length; i++){ %>
    <tr>
      <td><%= i + 1 %></td>
      <td><%= quantity[i].quantityName %></td>
      <td><%= quantity[i].quantityProduct %></td>
      <td>
        <form
          action="/quantity/delete/<%= quantity[i]._id %>?_method=DELETE"
          method="POST"
        >
          <a
            href="/quantity/edit/<%= quantity[i]._id %>"
            class="btn btn-info btn-sm"
          >
            <i class="fas fa-pencil-alt"></i>
            Ubah
          </a>
          <button class="btn btn-danger btn-sm">
            <i class="fas fa-trash"></i>
            Hapus
          </button>
        </form>
      </td>
    </tr>
    <% } %>
  </tbody>
</table>
